{
  "name": "vscode-commands-executor",
  "displayName": "Commands Executor",
  "description": "Enables the execution of vscode commands on open vscode via the vscode:// URI and keyboard shortcuts.",
  "version": "1.0.1",
  "publisher": "ionutvmi",
  "engines": {
    "vscode": "^1.63.0"
  },
  "author": {
    "name": "Mihai Vilcu"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "CLI",
    "Command Line",
    "Terminal",
    "Commands"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "bugs": {
    "url": "https://github.com/ionutvmi/vscode-commands-executor/issues",
    "email": "ionutvmi@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ionutvmi/vscode-commands-executor.git"
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "1.73.1",
    "@types/glob": "8.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "8.28.0",
    "typescript": "4.9.3"
  }
}
